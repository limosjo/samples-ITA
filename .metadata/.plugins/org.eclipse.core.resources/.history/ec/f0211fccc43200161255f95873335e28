package com.mnl.app.my_app_simple;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        System.out.println( "Hello World!" );
        add(1,3);
       
    }
    public static int add(int i, int j){
    	int sum=0;
    	sum = i +j;
    	return sum;
    }
}
